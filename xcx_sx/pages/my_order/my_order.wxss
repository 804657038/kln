@import "../../utils/font.wxss";

page{
  background-color: #f6f6f6;
}
/*nav  */
.nav-list{
  display: flex;
  align-items: center;
  justify-content: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  height: 100rpx;
  background-color: #fff;

}
.nav-item{
 font-size: 28rpx;
 color: #333;
 width: 50%;
 text-align: center;
}
.nav-item.cloors{
  color: #c9ab79;
}
/*--contents  */
.header-wrap{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  height: 90rpx;
  border-bottom: 1px solid #eee;
  margin-bottom: 30rpx;
}
.header-left{
  font-size: 28rpx;
  color: #333;
}
.header-right{
  font-size: 28rpx;
  color: #999;
}
.goods:last-child{
  margin-bottom: 0;
}
.look-sx{
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  border-top:1px solid #eee;
}
.look-sx-inner{
  font-size: 30rpx;
  color: #333;
}
.look-sx-inner-p{
  display: inline-block;
  font-size: 30rpx;
  color: #c9ab79;
}
.look-sx-inner-btn{
  width:180rpx;
  height: 70rpx;
  background-color: #c9ab79;
  display: flex;
  align-items: center;
  justify-content: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  font-size: 28rpx;
  color: #ffffff;
  border-radius: 3px;
}
.goods{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  margin-bottom: 30rpx;
}
.goods_wrap{
  background-color: #fff;
  margin-top: 20rpx;
  padding: 30rpx 50rpx 0;
}
.goods-pic{
  width: 160rpx;
  height: 160rpx;
  border-radius: 5px;
}
.goods-mid{
  display: flex;
  flex-flow: row wrap;
  align-content:space-around;
  height:80px;
}
.goods-mid-ne{
  width: 100%;
  font-size:28rpx;
  color: #333333
}
.goods-mid-money{
  font-size: 30rpx;
  color: #333;
}
.goods-right{
  display: flex;
  align-items:flex-end;
  height: 65px;
}
