@import "../../utils/font.wxss";
page{
  background-color: #f6f6f6;
}
.wrap{
  margin-bottom: 10rpx;
}
.header{
  height: 100rpx;
  background-color: #fff;
  display: flex;
  -webkit-display: flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
  align-items: center;
  -webkit-align-items: center;
  padding:0 40rpx;
  box-sizing: border-box;
}
.header-p{
  font-size: 28rpx;
  color:#333333;
}
/*下拉  */
.contents-wrap{
  padding:0 40rpx;
  background-color: #fff;
}
.contents{
  padding:40rpx 0;
  border-top:1px solid #eeeeee;
  background-color: #fff;
  font-size: 28rpx;
  color: #888888;
}
.contents-inner{
  margin-bottom: 10rpx;
}
.show_x{
  display: none;
}
/*过渡  */
.icon_s1{
  transition: 0.5s;
  transform:rotate(180deg);
}
.icon_s2{
  transition: 0.5s;
  transform:rotate(0deg);
}