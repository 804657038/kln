  margin: 0 auto;
}
.box{
  padding:0 40rpx;
}
.trans-operation-wrapper{
  font-size: 32rpx;
  color: #c9ab79;
  border-bottom: 1px solid #dddddd;
  padding-bottom: 30rpx;
}
.content{
  font-size: 28rpx;
  color: #333;
  padding-top: 40rpx;
  padding-bottom: 90rpx;
}