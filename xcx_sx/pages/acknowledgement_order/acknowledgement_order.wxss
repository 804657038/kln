@import "../../utils/font.wxss";

page{
  background-color: #f6f6f6;
}
.wrap{
    padding:30rpx 50rpx 30rpx;
    background-color: #fff;
}
.header{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  padding: 25rpx 0;
  border-bottom: 1px solid #ddd;
 
}
.header-left{
  font-size: 28rpx;
  color: #333;
}
.icon-xie-x{
  color: #c9ab79;
}
/*main  */
.inner-wrap{
  padding-top: 10rpx;
}
.inner-wrap .inner-one{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  margin: 30rpx auto;
}
.inner-one-left{
  font-size: 28rpx;
  color: #333333;
}
.inner-one-right{
  font-size: 28rpx;
  color: #333333;
}
.inner-down{
  font-size: 28rpx;
  color: #333;
}
/*--contents  */
.goods:last-child{
  margin-bottom: 0;
}
.goods{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  margin-bottom: 30rpx;
}
.goods_wrap{
  background-color: #fff;
  margin-top: 10rpx;
  padding: 30rpx 50rpx;
}
.goods-pic{
  width: 160rpx;
  height: 160rpx;
  border-radius: 5px;
}
.goods-mid{
  display: flex;
  flex-flow: row wrap;
  align-content:space-around;
  height:80px;
}
.goods-mid-ne{
  width: 100%;
  font-size:28rpx;
  color: #333333
}
.goods-mid-money{
  font-size: 30rpx;
  color: #333;
}
.goods-right{
  display: flex;
  align-items:flex-end;
  height: 65px;
}
/*--list  */
.list-wrap{
  padding:30rpx 50rpx;
  background-color: #fff;
  margin-top: 10rpx;
}
.list:last-child{
  margin-bottom: 0;
}
.list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  margin-bottom: 50rpx;
}
.list-left{
  font-size: 28rpx;
  color: #333;
}
.list-right{
  font-size: 28rpx;
  color: #333;
}
/*go-ls  */
.go-ls{
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
  width: 100%;
  height: 110rpx;
  position: fixed;
  bottom: 0;
}
.go-price{
  display: flex;
  align-items: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  width: 60%;
  height: 100%;
  background-color: #fff;
  padding-left: 50rpx;
}
.go-btn{
  width: 40%;
  height: 100%;
  background-color: #c9ab79;
  display: flex;
  align-items: center;
  justify-content:center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  font-size: 32rpx;
  color: #ffffff;
}
.total{
  font-size: 28rpx;
  color: #333333;
}
.money{
  font-size: 36rpx;
  color: #c9ab79;
}
.mask-wrap{
  position: absolute;
  bottom:-10000rpx;
  background:rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
}
.mask{
  position: absolute;
  bottom: 0;
  background-color: #fff;
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;

}
.select-mask-header{
  padding: 70rpx 0 40rpx;
  font-size: 40rpx;
  color: #333333;
  border-bottom: 1px solid #ddd;
}
.select-list{
  padding-bottom: 90rpx;
}
.select-item{
  display: flex;
  align-items: center;
  margin-top: 70rpx;
  position: relative;
}
.pic_mask{
  width: 80rpx;
  height: 80rpx;
  margin-left: 75rpx;
  border-radius: 5px;
}
.type_name{
  font-size: 28rpx;
  margin-left: 20rpx;
}
.position-radio{
  position: absolute;
  left: 0;
  width: 100%;
  height: 100%;
   display: flex;
  align-items: center;
  -webkit-display: flex;
  -webkit-align-items: center;
}
.tianjia{
  bottom:110rpx !important;
  transition: 0.5s;
}
.mask-close{
  height: 60%;
}