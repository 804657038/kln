@import"../../utils/font.wxss";
.icon-left{
  color: #555;
    font-size: 26rpx;
}
.nav-mid{
  font-family:"Arial";
  font-size: 34rpx;
}
.icon-right{
  color: #555;
   font-size: 38rpx;

}
.nav-wrap{
    display: flex;
    align-items: center;
    justify-content:space-between;
    -webkit-display: flex;
    -webkit-align-items: center;
     -webkit-justify-content:space-between;
    padding: 25rpx 30rpx;
    width: 100%;
    background-color: #fff;
    box-sizing:border-box;
    height: 100rpx;
    position: fixed;
    top:0;
    border-radius: 5px;
}
.main{
    background: #f6f6f6;
    width: 100%;
    height: 100%;
    position: fixed;
    top:55px;
    overflow: auto;
}
/*通知  */
.notice-wrap{
  width: 90%;
  margin: 0 auto 30rpx;
}
.time{
  padding: 20rpx 0; 
  font-size: 22rpx;
  color: #666666;
  text-align: center;
}
.notice-box{
  background-color: #fff;
}
.title{
  padding-top:30rpx;
  text-align: center;
  font-size: 26rpx;
  color: #666;
  font-weight: bold;
}
.content{
  padding:30rpx;
  text-indent: 30rpx;
  font-size: 26rpx;
  color: #666;
  border-bottom: 1px solid #e5e5e5
}
.look-con{
  height: 70rpx;
   display: flex;
  align-items: center;
  justify-content:center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content:center;
  font-size: 26rpx;
  color: #00a6a0;
}