@import"../../utils/font.wxss";
.icon-left{
  color: #555;
    font-size: 26rpx;
}
.nav-mid{
  font-family:"Arial";
  font-size: 34rpx;
}
.icon-right{
  color: #555;
   font-size: 26rpx;

}
.nav-wrap{
    display: flex;
    align-items: center;
    justify-content:space-between;
    -webkit-display: flex;
    -webkit-align-items: center;
     -webkit-justify-content:space-between;
    padding: 25rpx 30rpx;
    width: 100%;
    background-color: #fff;
    box-sizing:border-box;
    height: 100rpx;
    position: fixed;
    top:0;
    z-index: 10000000;
}
.main{
    background: #f6f6f6;
    width: 100%;
    height: 100%;
    position: fixed;
    top:100rpx;
    overflow:auto;
}
/*list  */
.stration-list{
  display: block;
  margin-bottom: 10rpx;
}
.stration-header{
    display: flex;
    align-items: center;
    justify-content:space-between;
    -webkit-display: flex;
    -webkit-align-items: center;
    -webkit-justify-content:space-between;
    height: 70rpx;
    background-color: #fff;
    padding: 0 30rpx;
    border-top: 1px solid #f6f6f6;
 
}
.stration-header-left{
  font-size: 26rpx;
  color: #666;
}
.stration-header-right{
  font-size: 26rpx;
  color: #c8c8c8;
}
/*删除  */
.containers{
  background-color: #fff;
}
.touch-item {
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  border-top:1px solid  #f6f6f6;
  width: 100%;
  overflow: hidden;
}

.content {

  width: 100%;

  padding: 10px;

  line-height: 22px;

  margin-right:0;

  -webkit-transition: all 0.4s;

  transition: all 0.4s;

  -webkit-transform: translateX(90px);

  transform: translateX(90px);

  margin-left: -90px;
  display: flex;
  align-items: center;
  -webkit-display: flex;
  -webkit-align-items: center;
}

.del {

  background-color: #00a6a0;

  width: 90px;

  display: flex;

  flex-direction: column;

  align-items: center;

  justify-content: center;

  color: #fff;

  -webkit-transform: translateX(90px);

  transform: translateX(90px);

  -webkit-transition: all
0.4s;

  transition: all
0.4s;

}

.touch-move-active .content,

.touch-move-active .del {

  -webkit-transform: translateX(0);

  transform: translateX(0);
}
/*显示隐藏  */
.hidden{
  display: none;
}
.content-img{
  background-color: pink;
  width:80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.content-img-inner{
  margin-left: 30rpx;
  color:#333;
  font-size: 26rpx;
}