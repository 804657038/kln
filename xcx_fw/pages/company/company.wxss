  box-sizing: border-box;
}
.section{
  width: 85%;
  font-size: 26rpx;
  border-bottom: 1px solid #00a6a0;
  padding-left: 20rpx;

}
.iconfont-x{
  display: flex;
  align-items: center;
  justify-content: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  margin-right: 20rpx;
}