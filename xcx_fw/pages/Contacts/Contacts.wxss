@import"../../utils/font.wxss";
.icon-left{
  color: #555;
    font-size: 26rpx;
}
.nav-mid{
  font-family:"Arial";
  font-size: 34rpx;
}
.icon-right{
  color: #555;
   font-size: 38rpx;

}
.nav-wrap{
    display: flex;
    align-items: center;
    justify-content:space-between;
    -webkit-display: flex;
    -webkit-align-items: center;
     -webkit-justify-content:space-between;
    padding: 25rpx 30rpx;
    width: 100%;
    background-color: #fff;
    box-sizing:border-box;
    height: 100rpx;
    position: fixed;
    top:0;
}
.main{
    background: #f6f6f6;
    width: 100%;
    height: 100%;
    position: fixed;
    top:55px;
    overflow: auto;
}
.contacts-list{
  padding-left: 30rpx;
  height: 50rpx;
  color: #666666;
  font-size: 22rpx;
  display: flex;
  align-items: center;
  -webkit-display: flex;
  -webkit-align-items: center;
}
.contacts-box{
  background-color: #fff;
  padding: 25rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
}
.contacts-box-inder{
  display: flex;
  align-items: center;
  -webkit-display: flex;
  -webkit-align-items: center;
}
.contacts-user{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
}
.contacts-user-img{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
}
.contacts-name{
  margin-left: 30rpx;
  font-size: 26rpx;
  color: #333333;
}
.contacts-adds{
  padding: 15rpx 30rpx;
  background-color: #00a6a0; 
  font-size: 26rpx;
  color: #fff;
}
/*shousuo  */
.suosuo-wrap{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  background-color: #feffff;
  opacity: 0.92;
  padding: 0 30rpx;

}
.suosuo{
  display: flex;
  -webkit-display: flex;
  box-sizing: border-box;
}
.section{
  width: 85%;
  font-size: 26rpx;
  border-bottom: 1px solid #00a6a0;
  padding-left: 20rpx;

}
.iconfont-x{
  display: flex;
  align-items: center;
  justify-content: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  margin-right: 20rpx;
}
/*弹窗  */
.mask-tan-wrap{
  width: 100%;
  height: 100%;
  background:rgba(0, 0, 0, 0.3);
  position: fixed;
  top:0;
}
.mask-tan{
  position:absolute;
  bottom:0;
  width: 100%;
}
.mask-btn{
  height: 100rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  -webkit-display: flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  border-bottom: 1px solid #f6f6f6;
  color: #333333;
  font-size: 26rpx;
}